{"query CharacterQuery($id: ID!) {\n  character(id: $id) {\n    ...CharacterFragment\n    location {\n      ...CharacterLocationFragment\n      __typename\n    }\n    origin {\n      ...CharacterLocationFragment\n      __typename\n    }\n    episode {\n      ...EpisodeFragment\n      __typename\n    }\n    __typename\n  }\n}\n\nfragment CharacterFragment on Character {\n  id\n  name\n  status\n  species\n  gender\n  image\n  created\n  __typename\n}\n\nfragment CharacterLocationFragment on Location {\n  id\n  name\n  type\n  dimension\n  created\n  __typename\n}\n\nfragment EpisodeFragment on Episode {\n  id\n  name\n  air_date\n  episode\n  characters {\n    ...CharacterFragment\n    __typename\n  }\n  created\n  __typename\n}\n":1,"query CharactersQuery($page: Int, $filter: FilterCharacter) {\n  characters(page: $page, filter: $filter) {\n    info {\n      count\n      pages\n      next\n      prev\n      __typename\n    }\n    results {\n      ...CharacterFragment\n      __typename\n    }\n    __typename\n  }\n}\n\nfragment CharacterFragment on Character {\n  id\n  name\n  status\n  species\n  gender\n  image\n  created\n  __typename\n}\n":2}